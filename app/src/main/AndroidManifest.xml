<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cafedroid.project10">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GridActivity" />
        <activity
            android:name=".SquareOne"
            android:label="@string/square_one" />
        <activity
            android:name=".FootballActivity"
            android:label="@string/football" />
        <activity
            android:name=".QuizActivity"
            android:windowSoftInputMode="stateHidden" />
        <activity android:name=".ReportCard" />
        <activity android:name="com.cafedroid.musicapp.MusicActivity" />
        <activity android:name="com.cafedroid.musicapp.albums" />
        <activity android:name="com.cafedroid.musicapp.artists" />
        <activity android:name="com.cafedroid.musicapp.playlists" />
        <activity android:name="com.cafedroid.musicapp.tracks" />
        <activity android:name="com.cafedroid.musicapp.NowPlaying"></activity>
    </application>

</manifest>